<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yibh.fourgank">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".FourApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".yonionyy.activity.YLambdaActivity"></activity>
        <activity android:name=".yonionyy.activity.BaseActivity"></activity>
        <activity android:name=".yonionyy.activity.YMainActivity"></activity>
        <activity android:name=".yonionyy.activity.MvpLoginActivity"></activity>
        <activity android:name=".yonionyy.activity.GankTestActivity"></activity>
        <activity android:name=".yonionyy.activity.RetrofitTestActivity"></activity>
    </application>

</manifest>